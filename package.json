{
  "name": "typescript-redux-systemjs",
  "version": "0.0.1",
  "description": "A TypeScript starter project for Redux and React applications.",
  "scripts": {
    "start": "concurrently --kill-others 'npm run dev'",
    "dev": "npm-run-all -p dev:*",
    "dev:browser-sync": "browser-sync start --server --files \"index.html\" \"styles.css\" \"js\" --no-inject-changes",
    "dev:tsc-watch": "tsc -w",
    "dev:json-server": "json-server --watch --port 3010 db.json",
    "build": "tsc"
  },
  "pre-push": [
    "build"
  ],
  "author": "Eric W. Greene",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "2.18.8",
    "concurrently": "3.4.0",
    "json-server": "0.9.6",
    "npm-run-all": "4.0.2",
    "pre-push": "0.1.1",
    "typescript": "2.2.1"
  },
  "dependencies": {
    "@types/react": "15.0.16",
    "@types/react-dom": "0.14.23",
    "normalize.css": "5.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "redux": "3.6.0",
    "systemjs": "0.20.10"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "private": true,
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/react/issues"
  },
  "homepage": "https://github.com/daggerok/react#readme"
}
