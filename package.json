{
  "name": "react-tsx",
  "version": "0.0.1",
  "description": "react typescript and webpack 2",
  "scripts": {
    "serve": "npm-run-all build lite-server",
    "build": "npm run webpack -- --env production -p",
    "start": "concurrently 'npm run dev' --kill-others",
    "dev": "npm-run-all -p webpack-watch lite-server",
    "webpack-watch": "npm run webpack -- -w --env development",
    "lite-server": "lite-server",
    "prewebpack": "rimraf -rf ./dist",
    "webpack": "webpack --progress"
  },
  "pre-push": [
    "webpack",
    "build"
  ],
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "concurrently": "3.4.0",
    "css-loader": "0.27.3",
    "file-loader": "0.10.1",
    "lite-server": "2.3.0",
    "npm-run-all": "4.0.2",
    "postcss": "5.2.16",
    "postcss-loader": "1.3.3",
    "pre-push": "0.1.1",
    "precss": "1.4.0",
    "rimraf": "2.6.1",
    "style-loader": "0.14.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-loader": "2.0.2",
    "typescript": "2.2.1",
    "url-loader": "0.5.8",
    "webpack": "2.2.1"
  },
  "dependencies": {
    "@types/react": "15.0.16",
    "@types/react-dom": "0.14.23",
    "bootswatch": "3.3.7",
    "react": "15.4.2",
    "react-dom": "15.4.2"
  },
  "main": "index.js",
  "private": true,
  "license": "ISC",
  "keywords": [
    "react",
    "typescript",
    "webpack 2"
  ],
  "author": "Maksim Kostromin <daggerok@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/react/issues"
  },
  "homepage": "https://github.com/daggerok/react#readme"
}
