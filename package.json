{
  "name": "daggerok.github.io.react",
  "version": "0.0.1",
  "description": "daggerok.github.io.react",
  "main": "./src/main.js",
  "scripts": {
    "serve": "browser-sync start -c ./config/bs-config.js",
    "prebuild": "npm run prewebpack && npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.production.babel.js -c --process -p",
    "dev": "npm run webpack -- -w",
    "start": "webpack-dev-server -d --config ./config/webpack.config.babel.js --content-base=./",
    "webpack": "webpack --config ./config/webpack.config.babel.js -c --process",
    "prewebpack": "npm i --no-optional",
    "predev": "npm run prewebpack",
    "clean": "rimraf -rf ./dist index.html"
  },
  "pre-commit": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://githuib.com/daggerok/daggerok.github.io.react"
  },
  "keywords": [
    "react",
    "github pages"
  ],
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.24.0",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.24.0",
    "browser-sync": "2.18.8",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "3.2.4",
    "css-loader": "0.27.3",
    "extract-text-webpack-plugin": "<2",
    "file-loader": "0.10.1",
    "handlebars": "4.0.6",
    "handlebars-loader": "1.4.0",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "less": "2.7.2",
    "less-loader": "4.0.2",
    "opn-cli": "3.1.0",
    "postcss-loader": "1.3.3",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.1",
    "source-map-loader": "0.2.0",
    "style-loader": "0.16.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.8",
    "webpack": "<2",
    "webpack-dev-server": "<2"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "bootstrap": "3.3.7",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "<4.0.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "transform-class-properties"
    ]
  }
}
