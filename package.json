{
  "name": "react-typescript",
  "version": "0.0.1",
  "scripts": {
    "precompile": "rimraf -rf ./dist ./react",
    "compile": "webpack --config ./config/webpack.config.js",
    "watch": "cross-env NODE_ENV=development npm run compile -- -w",
    "webpack-dev-server": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js -w",
    "preopen-url": "sleep 3",
    "open-url": "open-url http://localhost:8000/react/",
    "start": "npm-run-all -p webpack-dev-server open-url",
    "build": "cross-env NODE_ENV=production npm run compile"
  },
  "pre-push": [
    "compile",
    "build"
  ],
  "dependencies": {
    "@types/jquery": "2.0.40",
    "@types/react": "15.0.14",
    "@types/react-dom": "0.14.23",
    "bootstrap": "3.3.7",
    "jquery": "3.1.1",
    "react": "15.4.2",
    "react-dom": "15.4.2"
  },
  "devDependencies": {
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "3.2.3",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "npm-run-all": "4.0.2",
    "open-url": "2.0.2",
    "pre-push": "^0.1.1",
    "rimraf": "2.6.1",
    "source-map-loader": "0.1.6",
    "ts-loader": "2.0.1",
    "typescript": "2.2.1",
    "webpack": "1.14",
    "webpack-dev-server": "1.16"
  },
  "description": "react using typescript",
  "main": "./src/main.jsx",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "author": "Maksim Kostromin>",
  "bugs": {
    "url": "https://github.com/daggerok/react/issues"
  },
  "homepage": "https://github.com/daggerok/react#readme"
}
