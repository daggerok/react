{
  "name": "complete-react",
  "version": "0.0.1",
  "license": "ISC",
  "private": true,
  "scripts": {
    "sleep": "sleep 1",
    "url": "open-url localhost:3000",
    "serve": "npm-run-all build server",
    "babel-watch": "npm run babel -- -w",
    "stylus-watch": "npm run stylus -- -w",
    "babel": "babel ./src --minified -o ./app.js",
    "build": "npm-run-all stylus babel html:prod",
    "stylus": "stylus ./src/main.styl -o ./app.css",
    "postinstall": "bower install jquery bootstrap",
    "prestart": "cp -Rf ./index.dev.html ./index.html",
    "start": "npm-run-all -p stylus server babel-watch",
    "html:prod": "cp -Rf ./index.prod.html ./index.html",
    "server": "lite-server --config ./config/lite-server.js"
  },
  "config": {
    "bower_directory": "vendors"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "bower": "^1.7.9",
    "concurrently": "^3.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "editorconfig": "^0.13.2",
    "eslint": "^3.8.1",
    "eslint-config-stylelint": "^5.0.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-react": "^6.4.1",
    "lite-server": "^2.2.2",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "stylus": "^0.54.5",
    "webpack": "^1.13.3"
  },
  "babel": {
    "presets": [
      "stage-0",
      "react",
      "es2015"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "description": "small-valery",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Maksim Kostromin",
  "bugs": {
    "url": "https://github.com/daggerok/react/issues"
  },
  "homepage": "https://github.com/daggerok/react#readme"
}
