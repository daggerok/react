{
  "name": "react-router-4",
  "version": "0.0.1",
  "description": "react router 4",
  "main": "webpack.config.js",
  "author": "daggerok",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others --name=dev 'npm run webpack-dev-server'",
    "webpack-dev-server": "webpack-dev-server --env development --profile --colors",
    "prewebpack": "rimraf -rf ./dist",
    "webpack": "webpack --colors --progress",
    "watch": "npm run webpack -- --env development -w --profile",
    "build": "npm run webpack -- --env production",
    "ghpages": "npm run webpack -- --env ghpages",
    "serve": "concurrently --kill-others 'npm run lite-server'",
    "preserve": "npm run build",
    "lite-server": "lite-server"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "base-href-webpack-plugin": "^1.0.2",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.4.0",
    "css-loader": "^0.27.3",
    "html-webpack-plugin": "^2.28.0",
    "lite-server": "^2.3.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "postcss-sorting": "^2.0.1",
    "postcss-svgo": "^2.1.6",
    "react-hot-loader": "1.3.1",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "style-loader": "^0.14.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  }
}
