{
  "name": "top-5-tube",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "sleep": "sleep 3",

    "clean": "rimraf -rf ./dist",
    "predev": "npm run clean",
    "prebuild": "npm run clean",

    "prestart-server": "mv -f ./dist ./react; mkdir -p dist; mv -f react dist/",
    "start-server": "browser-sync start -c ./config/bs-config.js",

    "watch": "npm run webpack -- -w",
    "webpack": "NODE_ENV=development webpack --config=./config/webpack/dev.babel.js -c --process --profile --display-modules #--display-reasons",
    "dev": "npm-run-all -p watch json-server",

    "open-dev": "open-url localhost:8000/react/",
    "open-dev-url": "npm-run-all sleep open-dev",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "server.start": "npm-run-all -p webpack-dev-server server open-dev-url",
    "start": "npm-run-all -p webpack-dev-server open-dev-url",

    "open-prod": "open-url localhost:8000/react/",
    "open-prod-url": "npm-run-all sleep open-prod",
    "start-server-and-open-prod-url": "npm-run-all -p open-prod-url server",
    "build": "NODE_ENV=production webpack --config=./config/webpack/prod.babel.js -c --progress --profile --bail -p",
    "serve": "npm-run-all build start-server",

    "test": "npm run lint",
    "lint": "eslint ./src ./config -c ./config/.eslintrc.js --ext js 'src/**/*.js'"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "react-router-scroll": "^0.4.0",

    "youtube-api-search": "^0.0.5",
    "lodash": "^4.17.2",

    "react-http-request": "^1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "browser-sync": "^2.17.5",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.8.2",
    "editorconfig": "^0.13.2",
    "eslint": "^3.12.0",
    "eslint-config-stylelint": "^6.0.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "http-proxy-middleware": "^0.17.2",
    "image-webpack-loader": "^3.0.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.1",
    "open-url": "^2.0.2",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.0.0",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "to-string-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "react",
      "es2015"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "description": "small-valery",
  "main": "src/main.jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Maksim Kostromin",
  "bugs": {
    "url": "https://github.com/daggerok/react/issues"
  },
  "homepage": "https://github.com/daggerok/react#readme"
}
