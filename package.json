{
  "name": "react-tsx",
  "version": "0.0.1",
  "description": "react typescript and webpack 2",
  "scripts": {
    "precompile": "rimraf -rf ./dist ./react",
    "compile": "webpack --progress --display-error-details",

    "start": "webpack-dev-server --env development",

    "build": "npm run compile -- --env production -p",

    "serve": "npm-run-all build lite-server",
    "lite-server": "lite-server"
  },
  "pre-push": [
    "compile",
    "build"
  ],
  "dependencies": {
    "@types/core-js": "0.9.35",
    "@types/react": "15.0.14",
    "@types/react-dom": "0.14.23",
    "babel-polyfill": "6.23.0",
    "core-js": "2.4.1",
    "bootswatch": "3.3.7",
    "normalize.css": "5.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "ts-helpers": "1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "base-href-webpack-plugin": "1.0.2",
    "css-loader": "0.26.4",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "lite-server": "2.3.0",
    "npm-run-all": "4.0.2",
    "postcss": "5.2.16",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "pre-push": "0.1.1",
    "script-ext-html-webpack-plugin": "1.7.1",
    "style-loader": "0.13.2",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-loader": "2.0.1",
    "typescript": "2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "main": "index.js",
  "private": true,
  "license": "ISC",
  "keywords": [
    "react",
    "typescript",
    "webpack 2"
  ],
  "author": "Maksim Kostromin <daggerok@gmail.com>"
}
