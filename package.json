{
  "name": "react-typescript",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf -rf ./dist",
    "precompile": "npm i --no-optional",
    "wait-and-open-url": "sleep 3; open-url http://localhost:8000/react/",
    "start": "npm-run-all -p wait-and-open-url webpack-dev-server",
    "webpack-dev-server": "webpack-dev-server --config ./config/webpack.js -w --hot",
    "compile": "webpack --config ./config/webpack.js",
    "watch": "npm run compile -- -w",
    "build": "NODE_ENV=production npm run compile -- -p"
  },
  "dependencies": {
    "@types/jquery": "2.0.34",
    "@types/react": "0.14.55",
    "@types/react-dom": "0.14.19",
    "bootstrap": "3.3.7",
    "jquery": "3.1.1",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "2.2.4",
    "connect-history-api-fallback": "^1.3.0",
    "html-webpack-plugin": "2.24.1",
    "http-proxy-middleware": "^0.17.3",
    "npm-run-all": "3.1.2",
    "open-url": "^2.0.2",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.5",
    "ts-loader": "^1.3.3",
    "typescript": "2.1.4",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "description": "react-typescript",
  "main": "src/main.jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "author": "Maksim Kostromin",
  "bugs": {
    "url": "https://github.com/daggerok/react/issues"
  },
  "homepage": "https://github.com/daggerok/react#readme"
}
