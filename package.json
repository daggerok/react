{
  "name": "react-master",
  "version": "0.0.1",
  "main": "./config/webpack.config.js",
  "scripts": {
    "precompile": "rimraf -rf ./dist ./react",
    "compile": "webpack --config ./config/webpack.config.js --progress --display-error-details",

    "build": "npm run compile -- --env production",

    "preserve": "npm run build",
    "serve": "npm run server",
    "server": "browser-sync start -c ./config/bs/bs-config.js",

    "ghpages": "npm run compile -- --env ghpages",
    "postghpages": "mv -f ./dist ./react; mkdir -p ./dist/react; mv -f ./react ./dist/",

    "preghserve": "npm run ghpages",
    "ghserve": "npm run gh-server",
    "gh-server": "browser-sync start -c ./config/bs/bs-config.ghpages.js",

    "watch": "npm run compile -- --env development -w",
    "start": "webpack-dev-server -d --env development --config ./config/webpack.config.js --debug",

    "compose": "docker-compose --project-name=react -f ./config/docker-compose.yml",
    "precompose-up": "npm run build",
    "compose-up": "npm run compose -- up -d --remove-orphans",
    "compose-down": "npm run compose -- down --remove-orphans --rmi local -v"
  },
  "pre-push": [
    "compile",
    "ghpages",
    "build"
  ],
  "dependencies": {
    "bootswatch": "3.3.7",
    "core-js": "2.4.1",
    "es6-shim": "0.35.3",
    "jquery": "3.1.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-addons-pure-render-mixin": "15.4.2",
    "react-router": "3.0.2",
    "react-router-scroll": "0.4.1",
    "react-http-request": "1.0.3",
    "rx-connect": "0.6.1",
    "rx": "4.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "base-href-webpack-plugin": "1.0.2",
    "browser-sync": "2.18.8",
    "compression-webpack-plugin": "0.3.2",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.4",
    "cssnano": "3.10.0",
    "editorconfig": "0.13.2",
    "eslint": "3.17.1",
    "eslint-config-stylelint": "6.0.0",
    "eslint-config-xo": "0.17.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.6.3",
    "eslint-plugin-react": "6.10.0",
    "eslint-plugin-babel": "4.1.1",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "2",
    "file-loader": "0.10.1",
    "handlebars": "4.0.6",
    "handlebars-loader": "1.4.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "image-webpack-loader": "3.2.0",
    "imagemin": "5.2.2",
    "imagemin-mozjpeg": "6.0.0",
    "less": "2.7.2",
    "less-loader": "3.0.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "opn-cli": "3.1.0",
    "open-url": "2.0.2",
    "postcss-loader": "1.3.3",
    "pre-push": "0.1.1",
    "precss": "1.4.0",
    "progress-bar-webpack-plugin": "1.9.3",
    "rimraf": "2.6.1",
    "rucksack-css": "0.9.1",
    "sass-loader": "6.0.3",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "stylelint": "7.9.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "react",
      "es2015"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "syntax-dynamic-import",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://githuib.com/daggerok/daggerok.github.io.react"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "description": "react with webpack 2",
  "author": "Maksim Kostromin",
  "license": "ISC",
  "private": true
}
