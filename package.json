{
  "name": "small-valery",
  "version": "0.0.2",
  "license": "ISC",
  "scripts": {
    "gh-pages-deploy": "fish bin/gh-pages.fish",
    "postgh-pages-deploy": "open-url daggerok.github.io/react/",
    "sleep": "sleep 3",
    "clean": "rimraf -rf ./dist",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "prestart-server": "mv -f ./dist ./react; mkdir -p dist; mv -f react dist/",
    "start-server": "browser-sync start -c ./config/bs-config.js",
    "watch": "npm run webpack -- -w",
    "webpack": "cross-env NODE_ENV=development webpack --config=./config/webpack/dev.babel.js -c --process",
    "dev": "npm-run-all -p watch json-server",
    "open-dev": "open-url localhost:8000/react/",
    "open-dev-url": "npm-run-all sleep open-dev",
    "webpack-dev-server": "cross-env NODE_ENV=development webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "server.start": "npm-run-all -p webpack-dev-server server open-dev-url",
    "start": "npm-run-all -p webpack-dev-server open-dev-url",
    "open-prod": "open-url localhost:8000/react/",
    "open-prod-url": "npm-run-all sleep open-prod",
    "start-server-and-open-prod-url": "npm-run-all -p open-prod-url server",
    "build": "cross-env NODE_ENV=production webpack --config=./config/webpack/prod.babel.js -c --progress --profile --bail -p",
    "serve": "npm-run-all build start-server",
    "test": "npm run lint",
    "lint": "eslint ./src ./config -c ./config/.eslintrc.js --ext js 'src/**/*.js'",
    "disabled.test": "karma start ./config/karma.config.js --single-run",
    "disabled.tdd": "karma start ./config/karma.config.js",
    "disabled.e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-protractor",
    "pree2e": "webdriver-manager update",
    "prepare-e2e": "concurrently -p '[{name}]' -n 'HTTP' -c 'bgYellow.bold' 'npm run webpack-dev-server' --kill-others --success first",
    "wait-20s-and-run-protractor": "concurrently -p '[{name}]' -n 'PROTRACTOR' -c 'bgGreen.bold' 'npm-run-all wait-20s protractor' --kill-others --success first",
    "postwait-20s-and-run-protractor": "npm run stop",
    "wait-20s": "sleep 20",
    "protractor": "protractor ./config/protractor.js",
    "stop": "kill -9 $(ps waux | grep webpack-dev-server | grep -v color | awk '{print $2}') | true"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.1.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-addons-transition-group": "15.4.2",
    "react-router": "3.0.2",
    "react-router-scroll": "0.4.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.6",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "browser-sync": "2.18.8",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.2",
    "cssnano": "3.10.0",
    "editorconfig": "0.13.2",
    "eslint": "3.16.1",
    "eslint-config-stylelint": "6.0.0",
    "eslint-config-xo": "0.17.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-loader": "1.6.3",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "1",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.3",
    "image-webpack-loader": "3.2.0",
    "imagemin": "5.2.2",
    "imagemin-mozjpeg": "6.0.0",
    "json-loader": "0.5.4",
    "npm-run-all": "4.0.2",
    "open-url": "2.0.2",
    "postcss": "5.2.15",
    "postcss-loader": "1.3.3",
    "rimraf": "2.6.1",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "stylelint": "7.9.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.5.0",
    "to-string-loader": "1.1.5",
    "url-loader": "0.5.8",
    "webpack": "1",
    "webpack-dev-server": "1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "react",
      "es2015"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "description": "small-valery",
  "main": "src/main.jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/small-valery.github.com.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Maksim Kostromin",
  "bugs": {
    "url": "https://github.com/daggerok/small-valery.github.com/issues"
  },
  "homepage": "https://github.com/daggerok/small-valery.github.com#readme"
}
